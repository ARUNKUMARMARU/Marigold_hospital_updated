---
import Pagebanner from "../components/pagebanner.astro";
import Layout from "../layouts/Layout.astro";
import Special from "../components/special.jsx";


---

<Layout title="Best MultiSpeciality Hospital in Bangalore">
    <div class="section-pad">
        <div class="container">
            <Pagebanner title="Speciality Service" img1="/images/ICU12-min.webp" img2="/images/ICU3-min.webp" content="" />
        </div>
    </div>

    <section class="section-pad sp-wrap">
        <div class="bg">
            <img src="/bluewave.webp" alt="marigold-hospital-bluewave6">
        </div>
        <div class="container">
            <Special client:load />
        </div>
    </section>


    
  
</Layout>

<style>
    
    .sp-wrap{
        position: relative;
       
    }

</style>